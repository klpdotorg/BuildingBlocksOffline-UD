<?xml version='1.0' encoding='utf-8'?>
<widget id="com.Akshara.UDBuildingBlocks" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>UD-BuildingBlocks</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http:///" />
    <allow-intent href="https:///" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="21" />
        <!-- <preference name="android-targetSdkVersion" value="30" /> -->
        <icon density="mdpi" src="assets/mdpi/ic_akshara_logo.png" />
        <icon density="hdpi" src="assets/hdpi/ic_akshara_logo.png" />
        <icon density="xhdpi" src="assets/xhdpi/ic_akshara_logo.png" />
        <icon density="xxhdpi" src="assets/xxhdpi/ic_akshara_logo.png" />
        <allow-intent href="market:*" />
        <preference name="Fullscreen" value="false" />
        <preference name="Orientation" value="portrait" />
        <preference name="AndroidInsecureFileModeEnabled" value="true" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
